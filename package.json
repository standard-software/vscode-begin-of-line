{
  "name": "vscode-begin-of-line",
  "displayName": "Begin Of Line",
  "description": "insert a string on begin of line.",
  "version": "0.2.0",
  "publisher": "SatoshiYamamoto",
  "author": {
    "name": "Satoshi Yamamoto",
    "email": "standard.software.net@gmail.com",
    "url": "https://www.facebook.com/stndardsoftware"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/standard-software/vscode-begin-of-line"
  },
  "engines": {
    "vscode": "^1.62.1"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "keywords": [
    "indent",
    "reply mark",
    "template"
  ],
  "activationEvents": [
    "onCommand:BeginOfLine.InsertBeginLineAllLines",
    "onCommand:BeginOfLine.InsertBeginLineOnlyTextLines",
    "onCommand:BeginOfLine.InsertBeginLineOnlyMinIndent",
    "onCommand:BeginOfLine.InsertBeginTextAllLines",
    "onCommand:BeginOfLine.InsertBeginTextOnlyTextLines",
    "onCommand:BeginOfLine.InsertBeginTextOnlyMinIndent",
    "onCommand:BeginOfLine.InsertMinIndentAllLines",
    "onCommand:BeginOfLine.InsertMinIndentOnlyTextLines",
    "onCommand:BeginOfLine.DeleteBeginText"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "BeginOfLine.InsertBeginLineAllLines",
        "title": "Begin Of Line | Insert | All Lines"
      },
      {
        "command": "BeginOfLine.InsertBeginLineOnlyTextLines",
        "title": "Begin Of Line | Insert | Only Text Lines"
      },
      {
        "command": "BeginOfLine.InsertBeginLineOnlyMinIndent",
        "title": "Begin Of Line | Insert | Only Min Indent Text Lines"
      },
      {
        "command": "BeginOfLine.InsertBeginTextAllLines",
        "title": "Begin Of Line | Insert | All Lines | Begin Of Text"
      },
      {
        "command": "BeginOfLine.InsertBeginTextOnlyTextLines",
        "title": "Begin Of Line | Insert | Only Text Lines | Begin Of Text"
      },
      {
        "command": "BeginOfLine.InsertBeginTextOnlyMinIndent",
        "title": "Begin Of Line | Insert | Only Min Indent Text Lines | Begin Of Text"
      },
      {
        "command": "BeginOfLine.InsertMinIndentAllLines",
        "title": "Begin Of Line | Insert | All Lines | Min Indent"
      },
      {
        "command": "BeginOfLine.InsertMinIndentOnlyTextLines",
        "title": "Begin Of Line | Insert | Only Text Lines | Min Indent"
      },
      {
        "command": "BeginOfLine.DeleteBeginText",
        "title": "Begin Of Line | Delete | Begin Of Text"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Begin Of Line",
      "properties": {
        "BeginOfLine.insertString": {
          "type": "string",
          "default": "> ",
          "description": "Insert String"
        }
      },
      "default": {
        "insertString": "> "
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3"
  }
}
